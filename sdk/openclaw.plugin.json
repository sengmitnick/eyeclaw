{
  "id": "eyeclaw",
  "configSchema": {
    "type": "object",
    "properties": {
      "sdkToken": {
        "type": "string",
        "description": "SDK token for authentication"
      },
      "botId": {
        "type": "string",
        "description": "Bot ID in EyeClaw Rails app"
      },
      "serverUrl": {
        "type": "string",
        "description": "EyeClaw Rails server URL (e.g., http://localhost:3000)"
      }
    },
    "required": ["sdkToken", "botId", "serverUrl"]
  }
}
